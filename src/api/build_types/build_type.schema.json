{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "title": "Build Type",
  "description": "The type of build",
  "required": ["name"],
  "additionalProperties": false,
  "properties": {
    "id": {
      "$id": "#/properties/id",
      "type": "integer",
      "title": "The database ID of the build type."
    },
    "name": {
      "$id": "#/properties/name",
      "type": "string",
      "title": "The name of the build type."
    },
    "created_at": {
      "$id": "#/properties/created_at",
      "type": "string",
      "title": "The creation date of the build type."
    },
    "updated_at": {
      "$id": "#/properties/updated_at",
      "type": "string",
      "title": "The date the build type was last updated."
    },
    "deleted_at": {
      "$id": "#/properties/deleted_at",
      "type": "string",
      "title": "The date the build type was deleted."
    }
  }
}
