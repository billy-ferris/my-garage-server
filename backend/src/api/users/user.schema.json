{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "title": "User",
  "description": "The User",
  "required": ["email", "name", "password"],
  "additionalProperties": false,
  "properties": {
    "id": {
      "$id": "#/properties/id",
      "type": "integer",
      "title": "The database ID of the user."
    },
    "email": {
      "$id": "#/properties/email",
      "type": "string",
      "title": "The users email. Must be unique."
    },
    "name": {
      "$id": "#/properties/name",
      "type": "string",
      "title": "The users name."
    },
    "password": {
      "$id": "#/properties/password",
      "type": "string",
      "title": "The users hashed password."
    },
    "last_login": {
      "$id": "#/properties/last_login",
      "type": "string",
      "title": "The date of the user's last login."
    },
    "created_at": {
      "$id": "#/properties/created_at",
      "type": "string",
      "title": "The creation date of the user."
    },
    "updated_at": {
      "$id": "#/properties/updated_at",
      "type": "string",
      "title": "The date the user was last updated."
    },
    "deleted_at": {
      "$id": "#/properties/deleted_at",
      "type": "string",
      "title": "The date the user was deleted."
    }
  }
}
