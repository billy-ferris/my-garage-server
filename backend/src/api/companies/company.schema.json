{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "title": "Company",
  "description": "The Company",
  "required": ["name"],
  "additionalProperties": false,
  "properties": {
    "id": {
      "$id": "#/properties/id",
      "type": "integer",
      "title": "The database ID of the Company."
    },
    "name": {
      "$id": "#/properties/name",
      "type": "string",
      "title": "The companies name."
    },
    "email": {
      "$id": "#/properties/email",
      "type": "string",
      "title": "The companies email."
    },
    "phone": {
      "$id": "#/properties/phone",
      "type": "string",
      "title": "The companies phone number."
    },
    "logo_url": {
      "$id": "#/properties/logo_url",
      "type": "string",
      "title": "The companies logo."
    },
    "description": {
      "$id": "#/properties/description",
      "type": "string",
      "title": "The companies description."
    },
    "website_url": {
      "$id": "#/properties/website_url",
      "type": "string",
      "title": "The companies website."
    },
    "address_id": {
      "$id": "#/properties/address_id",
      "type": "number",
      "title": "The database ID of the companies address."
    },
    "created_at": {
      "$id": "#/properties/created_at",
      "type": "string",
      "title": "The creation date of the company."
    },
    "updated_at": {
      "$id": "#/properties/updated_at",
      "type": "string",
      "title": "The date the company was last updated."
    },
    "deleted_at": {
      "$id": "#/properties/deleted_at",
      "type": "string",
      "title": "The date the company was deleted."
    }
  }
}
