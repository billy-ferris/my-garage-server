{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "title": "vehicle Info",
  "description": "The vehicle Info",
  "required": [
    "user_id",
    "vehicle_id",
    "ownership_status_id"
  ],
  "additionalProperties": false,
  "properties": {
      "id": {
        "$id": "#/properties/id",
        "type": "integer",
        "title": "The database ID of the vehicle."
      },
      "user_id": {
        "$id": "#/properties/user_id",
        "type": "integer",
        "title": "The id of the user."
      },
      "vehicle_id": {
        "$id": "#/properties/vehicle_id",
        "type": "integer",
        "title": "The id of the vehicle."
      },
      "color": {
        "$id": "#/properties/color",
        "type": "string",
        "title": "The color of the vehicle."
      },
      "purchase_date": {
          "$id": "#/properties/purchase_date",
          "type": "string",
          "format": "date-time",
          "title": "The date the vehicle was purchased."
      },
      "purchase_price": {
        "$id": "#/properties/purchase_price",
        "minimum": 0,
        "type": "number",
        "title": "The price the vehicle was purchased at."
      },
      "msrp": {
        "$id": "#/properties/msrp",
        "minimum": 0,
        "type": "number",
        "title": "The msrp of the vehicle."
      },
      "ownership_status_id": {
        "$id": "#/properties/ownership_status_id",
        "type": "number",
        "title": "The id of the vehicle ownership status."
      },
      "build_type_id": {
        "$id": "#/properties/build_type_id",
        "type": "number",
        "title": "The id of the vehicle build type."
      },
      "mileage_bought_at": {
        "$id": "#/properties/mileage_bought_at",
        "minimum": 0,
        "type": "number",
        "title": "The mileage the vehicle was bought at."
      },
      "mileage_at": {
        "$id": "#/properties/mileage_at",
        "minimum": 0,
        "type": "number",
        "title": "The mileage the vehicle is currently at."
      },
      "sold_date": {
        "$id": "#/properties/sold_date",
        "type": "string",
        "format": "date-time",
        "title": "The date the vehicle was sold."
    },
      "sold_price": {
        "$id": "#/properties/sold_price",
        "minimum": 0,
        "type": "number",
        "title": "The price the vehicle was sold at."
      },
      "created_at": {
        "$id": "#/properties/created_at",
        "type": "string",
        "title": "The creation date of the vehicle make in the database."
      },
      "updated_at": {
        "$id": "#/properties/updated_at",
        "type": "string",
        "title": "The date the vehicle make was last updated in the database."
      },
      "deleted_at": {
        "$id": "#/properties/deleted_at",
        "type": "string",
        "title": "The date the vehicle make was deleted in the database."
      }
  }
}